{% if items|length > 0 %}
<p class="dialog-title alert alert-warning txt-center">{{ questionMsg }}</p>
<div class="row">
    <p><button class="ui-button ui-widget ui-state-default ui-corner-all ui-button-text-only bb-revision-see-details">{{ 'validate_cancel_see_details'|trans }}</button></p>
    <div class="bb-revision-list" style="display:none;">
        <div class="bb5-list-data-toolbar bb5-toolbar-order">
            <div class="row">
                <p class="bb5-toolbar-order-col col-bb5-60"><a href="#">{{ 'title'|trans }}</a></p>
                <p class="bb5-toolbar-order-col col-bb5-20"><a href="#">{{ 'modified_at'|trans }}</i></a></p>
            </div>
        </div>

        {% if noteMsg %}
        <p class="alert alert-warning txt-center">{{ noteMsg }}</p>
        {% endif %}
        <ul class="bb5-list-data bb5-list-display-list clearfix bb-revision-selector">
            <input type="checkbox" name="test" checked style="margin-left:15px;" class="general-revision-input"/> {{ trans('select_all') }}
            {{ items }}
        </ul>
    </div>
    {% if displayUpdateDate == true %}
        <div class="update_date_page" style="float: right;">
            <input id="update_date" type="checkbox" name="update_date" style="width: 12px; height: 12px;" />
            <label for="update_date" style="font-size: 15px; color: #363636; font-weight: bold;">Mettre Ã  jour la date de publication</label>
        </div>
    {% endif %}
</div>
{% else %}
<p>{{ noContentMsg }}</p>
{% endif %}